<UserControl x:Class="BocchiTracker.Client.Views.UserConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BocchiTracker.Client.Views"
        mc:Ignorable="d"
        Height="450" Width="800">
    <StackPanel Background="{DynamicResource MaterialDesignPaper}">
        
        <GroupBox Header="Authentication"></GroupBox>
        
        <GroupBox Header="Misc">
            <StackPanel>
                <CheckBox FontSize="16" Foreground="White" VerticalContentAlignment="Bottom" Content="Enable Open WebBrowser" IsChecked="{Binding EnableOpenWebBrowser.Value}"></CheckBox>
            </StackPanel>
        </GroupBox>

    </StackPanel>
</UserControl>
